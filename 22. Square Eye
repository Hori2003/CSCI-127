import turtle

wn = turtle.Screen()            
wn.bgcolor("khaki")
turtle.colormode(255)

tess = turtle.Turtle()

tess.pensize(5)
tess.speed(0)

for i in range(36):
    tess.pencolor(0,i*7,0)
    tess.forward(10)
    tess.left(10)
    for i in range(4):
        tess.left(90)
        tess.forward(300)
        tess.backward(50)
